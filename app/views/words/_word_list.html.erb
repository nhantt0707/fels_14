<table class = "table table-striped table-bordered">
  <thead>
  	<th>English</th>
  	<th>Vietnamese</th>	
  </thead>
  <tbody>
  	<% @words.each do |word| %>
  		<tr>
  			<td><a href = "javascript:void(0);"><%= word.english %></a></td>
  			<td><%= word.word_answers.correct.content %></td>
  		</tr>
  	<% end %>
  </tbody>
</table>